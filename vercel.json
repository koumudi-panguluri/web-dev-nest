{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "vercel-sapper",
            "config": {
                "build": true
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}